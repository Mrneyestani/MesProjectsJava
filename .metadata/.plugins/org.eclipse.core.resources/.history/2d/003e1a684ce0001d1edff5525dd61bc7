package fr.doranco.exemple.entity;
import java.util.Date;

import fr.doranco.exemple.enums.EtatCommandeEnum;
import fr.doranco.exemple.enums.LivraisonCommandeEnum;
public class Commande {
	
	private int id; 
    private int numeroCommande;
    private Date dateLivraison;
    private Date dateCreation;
    private Date dateAnnulation;
    private LivraisonCommandeEnum livraison;
    private EtatCommandeEnum statut;

	
	public Commande() {
		
	}


	public Commande(int id, int numero_commande, Date date_livraison, Date date_creation, Date date_annulation,
			String mode_livraison, String statut) {
		super();
		this.id = id;
		this.numero_commande = numero_commande;
		this.date_livraison = date_livraison;
		this.date_creation = date_creation;
		this.date_annulation = date_annulation;
		this.mode_livraison = mode_livraison;
		this.statut = statut;
	}
	
	

}
