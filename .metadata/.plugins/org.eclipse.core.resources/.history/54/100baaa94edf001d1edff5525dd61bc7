package fr.doranco.exemple.main.adresse;

import fr.doranco.exemple.entity.Adresse;
import fr.doranco.exemple.model.dao.AdresseDao;
import fr.doranco.exemple.model.dao.IAdresseDao;
import fr.doranco.exemple.model.dao.IUserDao;
import fr.doranco.exemple.model.dao.UserDao;
import fr.doranco.exemple.utils.Dates;

public class MainAdd {


	public static void main(String[] args) {
		
		try {
			final IAdresseDao adresseDao = new AdresseDao();

			Adresse adresse = new Adresse("85", "des landes", "Chatou", "78400");
			int id = adresseDao.addAdresse(adresse);
			adresse.setId(id);
			System.out.println(adresse);
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

}
