public class MainDeleteCarte {


	public static void main(String[] args) {
		
		try {
			final ICartePaiementDao cartePaiementDao = new CarteDao();

			int id = 6;
			System.out.println("Supression d'une carte :");
			cartePaiementDao.removeCartePaiement(id);
