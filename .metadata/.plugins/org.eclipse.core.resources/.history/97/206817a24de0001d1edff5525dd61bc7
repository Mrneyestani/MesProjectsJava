package fr.doranco.exemple.model.dao;

import java.util.List;

import fr.doranco.exemple.entity.Commande;
import fr.doranco.exemple.entity.User;

public interface ICommandeDao {

	int addCommande(Commande commande) throws Exception;
	User getCommandeById(int id) throws Exception;
	void annulerCommande(Commande commande) throws Exception;
	void deleteUser(int id) throws Exception;
	List<Commande> getCommandes() throws Exception;
}


